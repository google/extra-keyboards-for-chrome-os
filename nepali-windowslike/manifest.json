{
  "name": "Nepali Windowslike",
  "version": "1",
  "manifest_version": 2,
  "description": "Nepali Windowslike keyboard layout for Chrome OS",
  "permissions": [
    "input"
  ],
  "input_components": [
    {
      "name": "Nepali Windowslike",
      "type": "ime",
      "id": "nepali-windowslike",
      "description": "Nepali Windowslike keyboard layout",  // user visible description
      "language": "ne-NP",  // The primary language this IME is used for
      "layouts": ["ne(windowslike)"]
    }
  ]
}
