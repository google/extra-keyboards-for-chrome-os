{
  "name": "ComposeKey",
  "version": "1.5",
  "manifest_version": 2,
  "description": "Compose Key for Chrome OS",
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "input",
    "storage"
  ],
  "input_components": [
    {
      "name": "Compose Key",
      "type": "ime",
      "id": "compose_key",
      "description": "Adds Compose key functionality to Chrome OS",
      "language": "en-US",
      "layouts": ["us"]
    }
  ],
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  }
}
